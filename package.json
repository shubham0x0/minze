    "expo-font": "^7.0.0",
    "expo-image-picker": "^7.0.0",
    "expo-location": "^7.0.0",
    "expo-web-browser": "^7.0.1",
    "formik": "^1.5.8",
    "graphql": "^14.5.8",
    "lodash": "^4.17.15",
    "lottie-ios": "3.1.3",
    "lottie-react-native": "^3.2.1",
    "prop-types": "15.7.2",
    "react": "16.8.6",
