    "npm-run-all": "4.1.5",
    "patch-package": "^6.1.4",
    "prettier": "1.18.2",
    "react-dom": "^16.11.0",
    "react-native-asset": "^2.0.0",
    "react-test-renderer": "16.8.6",
    "rimraf": "3.0.0",
